{
  "name": "cis-bv/netconf",
  "description": "A vendor-agnostic PHP NETCONF implementation",
  "license": "MIT",
  "version": "2.0.0",
  "require": {
    "php": "^8.3 || ^8.4",
    "ext-simplexml": "*",
    "phpseclib/phpseclib": "^3.0.0"
  },
  "require-dev": {
    "pestphp/pest": "^4.1"
  },
  "authors": [
    {
      "name": "Lamoni Finlayson",
      "email": "lamonif@gmail.com"
    },
    {
      "name": "Kay Schroeder",
      "email": "kay.schroeder@cis-gotha.de"
    }
  ],
  "homepage": "https://github.com/cis-ks/netconf",
  "keywords": [
    "php",
    "netconf"
  ],
  "autoload": {
    "psr-4": {
      "CisBv\\Netconf\\": "src",
      "Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
